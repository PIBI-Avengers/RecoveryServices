select (Select top 1 cnsmr_accnt_crdtr_rfrnc_id_txt from dma_oltp_SNAP.dbo.cnsmr_accnt with (nolock) where 1=1 and cnsmr_id = consumer_id) claimId
,'PRS - CRB' lobDescription
,payor_code clientCode
,kpiStatusDate
,dateadd(m,datediff(m,0,kpiStatusDate),0) dateNum
,amount
,'Placements (#)' inflectionPoint
from (select distinct
	ca.cnsmr_id as Consumer_ID
	,ca.cnsmr_accnt_id as Consumer_Account_ID
	,c.cnsmr_idntfr_agncy_id as DM9_Consumer_Number
	,ca.cnsmr_accnt_idntfr_agncy_id as DM9_Consumer_Account_Number
	,cast(ca.cb_rprtng_strt_dt as datetime) kpiStatusDate
	,case
		when cr.crdtr_nm like '%Equian%' then 'Equian'
		when cr.crdtr_nm like '%DM' then 'Data Mining'
		when ucai.UDEFSOURCE = 'DataMining' then 'Data Mining'
		else 'CRB'
		end as Recovery_Source
	,cr.crdtr_nm as Payor
	,cr.crdtr_shrt_nm as Payor_Code
	,isnull(ppn.PPN_Flag,0) as PPN_Flag
	,cast(1 as decimal(18,5)) as amount
from dma_oltp_SNAP.dbo.cnsmr (nolock) c
	inner join dma_oltp_SNAP.dbo.cnsmr_accnt (nolock) ca on c.cnsmr_id = ca.cnsmr_id
	inner join dma_oltp_SNAP.dbo.crdtr (nolock) cr on ca.crdtr_id = cr.crdtr_id
	inner join dma_oltp_SNAP.dbo.cnsmr_accnt_bal (nolock) cab1 on ca.cnsmr_accnt_id = cab1.cnsmr_accnt_id and cab1.bal_nm_id = 1
	left outer join dma_oltp_SNAP.dbo.UDEFCLIENTACCOUNTINFO (nolock) ucai on ca.cnsmr_accnt_id = ucai.cnsmr_accnt_id
	left outer join
		(select
			ca.cnsmr_accnt_id
			,caal.upsrt_dttm as CAAL_Date
			,caal.rslt_Cd
			,case
				when caal.rslt_Cd in (280,301,279,287,283,269,277,285,281,263,278,273) then 'Recall'
				when caal.rslt_Cd in (286,274,284,302,272,282) then 'Return'
				else 'Not Available'
				end as Closure_Category
			,cab1.cnsmr_accnt_bal_amnt + cab3.cnsmr_accnt_bal_amnt as Closure_Amount
			,row_number() over(partition by ca.cnsmr_accnt_idntfr_agncy_id order by caal.upsrt_dttm desc) as row_num
		from
			dma_oltp_SNAP.dbo.cnsmr_accnt_ar_log (nolock) caal
			inner join dma_oltp_SNAP.dbo.cnsmr (nolock) c on caal.cnsmr_id = c.cnsmr_id
			inner join dma_oltp_SNAP.dbo.cnsmr_accnt (nolock) ca on c.cnsmr_id = ca.cnsmr_id
			inner join dma_oltp_SNAP.dbo.crdtr (nolock) cr on ca.crdtr_id = cr.crdtr_id
			inner join dma_oltp_SNAP.dbo.cnsmr_accnt_bal (nolock) cab1 on ca.cnsmr_accnt_id = cab1.cnsmr_accnt_id and cab1.bal_nm_id = 1
			left outer join dma_oltp_SNAP.dbo.cnsmr_accnt_bal (nolock) cab3 on ca.cnsmr_accnt_id = cab3.cnsmr_accnt_id and cab3.bal_nm_id = 3
		where
			caal.cnsmr_accnt_ar_mssg_txt not like ('%FROM-VOX-TO-DM9%')
			and cab1.cnsmr_accnt_bal_amnt &lt;&gt; -cab3.cnsmr_accnt_bal_amnt
			and caal.actn_cd = 256
			and c.wrkgrp_id = 467)clo
		on ca.cnsmr_accnt_id = clo.cnsmr_accnt_id and clo.row_num = 1
	left outer join
		(select
			cat.cnsmr_accnt_id
			,1 as PPN_Flag
		from
			dma_oltp_SNAP.dbo.cnsmr_Accnt_Tag (nolock) cat
		where
			cat.tag_id = 752
			and cat.cnsmr_accnt_sft_delete_flg = 'n')ppn
		on ca.cnsmr_accnt_id = ppn.cnsmr_accnt_id
	where 1=1
	and ca.cb_rprtng_strt_dt &gt;= @startDate
	and ca.cb_rprtng_strt_dt &lt; @endDate
	and (clo.rslt_Cd &lt;&gt; 254 or clo.rslt_Cd is null))tbl1
where 1=1
and tbl1.PPN_Flag = 0
and Recovery_Source = 'crb'</property>

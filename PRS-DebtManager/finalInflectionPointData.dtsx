                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Preliminary = 1
-- Final = 0
declare @dateOffset int = 1
declare @startDate datetime = dateadd(m,datediff(m,0,getdate())-@dateOffset,0)
declare @endDate datetime = dateadd(m,1,@startDate)

select t.claimid
,concat('PRS - ',Recovery_Source) lobDescription
,payor_code clientCode
,kpiStatusDate
,@startDate dateNum
,'Inventory ($)' inflectionPoint
,cast(t.Current_Balance as decimal(18,5)) amount
from (select distinct
	ca.cnsmr_accnt_crdtr_rfrnc_id_txt claimId
	,ca.cnsmr_accnt_id
	,ca.cnsmr_accnt_idntfr_agncy_id as DM9_Account_Number
	,c.cnsmr_idntfr_agncy_id as DM9_Consumer_Number
	,case when OPinfo.UDEFRECSOURC = 'DataMining' then 'Data Mining' else 'CRB' end as Recovery_Source
	,cast(ca.cnsmr_accnt_plcmnt_date as datetime) kpiStatusDate
	,crdtr.crdtr_shrt_nm payor_code
	,CUR.cnsmr_accnt_bal_amnt as Current_Balance
	,row_number() over (partition by ca.cnsmr_accnt_id order by ca.cnsmr_accnt_id) as row_num
	from dma_oltp_SNAP.dbo.cnsmr (nolock) c 
	inner join dma_oltp_SNAP.dbo.cnsmr_accnt (nolock) ca on c.cnsmr_id = ca.cnsmr_id and CA.cnsmr_accnt_is_actv_per_crdtr_flg = 'Y'
	inner join dma_oltp_SNAP.dbo.crdtr (nolock) crdtr on ca.crdtr_id = crdtr.crdtr_id    
	inner join dma_oltp_SNAP.dbo.crdtr_Grp (nolock) CG on crdtr.crdtr_grp_id = CG.crdtr_grp_id
	inner join dma_oltp_SNAP.dbo.UDEFOVERPAYMENTINFO (nolock) OPinfo on OPinfo.cnsmr_accnt_id = CA.cnsmr_accnt_id   
	inner join dma_oltp_SNAP.dbo.cnsmr_accnt_bal (nolock) CUR on CUR.cnsmr_accnt_id = ca.cnsmr_accnt_id and CUR.bal_nm_id = 2
	inner join dma_oltp_SNAP.dbo.wrkgrp (nolock) w on c.wrkgrp_id = w.wrkgrp_id and w.wrkgrp_id &lt;&gt; '467' --Closed Accounts
	inner join dma_oltp_SNAP.dbo.cnsmr_accnt_bal (nolock) org on org.cnsmr_accnt_id = ca.cnsmr_accnt_id and org.bal_nm_id = 1
	inner join dma_oltp_SNAP.dbo.cnsmr_Wrk_actn (nolock) cwa on c.cnsmr_id = cwa.cnsmr_id
	left outer join dma_oltp_SNAP.dbo.UDEFCLAIMINFO (nolock) clm on ca.cnsmr_accnt_id = clm.cnsmr_accnt_id
	where 1=1
	and OPinfo.UDEFRECSOURC not in ('0','1','5'))t
where 1=1
and t.row_num = 1</property>

select *
from (select 
a16.ContractDescr clientCode
, case
	when a15.ContractDescr like '%cosmos%' then 'Cosmos'
	when a15.ContractDescr like '%unet%' then 'UNET'
	else 'Unknown'
	end platformName
,a11.ClaimID
,case
  when a15.[ContractCode] in ('113060004','113060005'
	,'113060006'
	,'113060007'
	,'113060010'
	,'113060011'
	,'113060012') then .5 * abs(a11.ProvPmts + a11.ProvAdjs) 
	else abs(a11.provpmts + a11.provadjs) 
	end amount
,'Credit Balance' lobDescription
,a11.DateStatusUpd kpiStatusDate
,@startDate dateNum
,case
	when ProvClmFinStatus = 'ACT' then 'Inventory - Active'
	when ProvClmFinStatus = 'APP' then 'Inventory - Approved'
	when ProvClmFinStatus = 'AWA' then 'Inventory - Awaiting Approval'

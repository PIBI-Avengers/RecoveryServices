                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Preliminary = 0
-- Final = 1
declare @dateOffset int = 0;
declare @startDate datetime = dateadd(m,datediff(m,0,getdate())-@dateOffset,0);
declare @endDate datetime = dateadd(m,1,@startDate);

select 	'Credit Balance' lobDescription
,dateadd(m,datediff(m,0,a11.insTxPostDate),0) dateNum
,'Fees - Adjustments' inflectionPoint
,a12.ClaimId
,a11.instxpostdate kpiStatusDate
,cast(a11.InsTxAmt  as numeric(21,5)) amount
,ram.WrittenBy
,ram.RAMWFullName
,a13.ContractDescr clientCode
,a12.paymentType recoveryTypeCode
,pt.PayTypeDescr recoveryType
,a111.TeamName username
,a111.TeamName firstName
,'' lastName
,a111.TeamName groupName
from safarirpts.dbo.vwMSTR_InsClmDet a11 with (nolock)
join safarirpts.dbo.vwMSTR_InsClm a12 with (nolock) on (a11.InsClmHdrKey = a12.InsClmHdrKey)
join safarirpts.dbo.vwMSTR_Contract a13 with (nolock) on (a12.ARContractKey = a13.ContractKey)
join safarirpts.dbo.vwMSTR_Provider    a15 with (nolock) on     (a12.ProvKey = a15.ProvKey)
join safarirpts.dbo.vwMSTR_Territory    a16 with (nolock) on     (a15.RegionKey = a16.TerritoryKey)
join safarirpts.dbo.vwMSTR_ProvRegion    a17 with (nolock)  on     (a16.RegionKey = a17.RegionKey)
join safarirpts.dbo.vwMSTR_TeamInsXref    a18 with (nolock) on     (a12.ARContractKey = a18.ContractKey)
join safarirpts.dbo. vwMSTR_Division    a110 with (nolock) on     (a17.DivKey = a110.DivKey)
join safarirpts.dbo.vwMSTR_Team    a111 with (nolock) on (a18.TeamKey = a111.TeamKey)
join safarirpts.dbo.vwMSTR_PaymentType pt with (nolock) on (a12.PaymentType = pt.PaymentType)
left join SafariRpts.dbo.vwMSTR_PatPolicy (nolock) pat on (a12.PatPolicyKey = pat.PatPolicyKey)
left join SafariRpts.dbo.vwMSTR_RAMWrittenBy2 (nolock) ram on (pat.WrittenBy = ram.WrittenBy)
where 1=1
and a12.ServiceType in ('CB', 'PPN')
and a13.ClientType in ('STD')
and a12.ARType in ('COM')
and a11.TxCode in ('DR-RA', 'ICR', 'IDR', 'RECODE', 'CR-RA', 'CR-INV', 'CREV-DISC', 'REBATE')
and a11.InsTxPostDate&gt;=@startDate

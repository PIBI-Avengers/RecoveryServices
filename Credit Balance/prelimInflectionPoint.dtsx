                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- credit balance inventory
-- credit balance recovery
-- credit balance closure
-- credit balance identified

-- Preliminary = 0
-- Final = 1
declare @dateOffset int = 0
declare @startDate datetime = dateadd(m,datediff(m,0,getdate())-@dateoffset,0)
declare @endDate datetime = dateadd(m,1,@startdate)

select 	'Credit Balance' lobDescription
,dateadd(m,datediff(m,0,pcl.recoveryIdDate),0) dateNum
,'Identifications ($)' inflectionPoint
, pcl.ClaimId
,pcl.RecoveryIdDate kpiStatusDate
,case when ctr.ContractCode in ('113060004', '113060005', '113060006', '113060007', '113060010', '113060011', '113060012')
	then pcd.OrigAmt * 0.5
	else pcd.OrigAmt
	end amount
,ram.WrittenBy
,ram.RAMWFullName
,ctr.ContractDescr clientCode
,pcl.paymentType recoveryTypeCode
,pay.PayTypeDescr recoveryType
,a111.TeamName username
,a111.TeamName firstName
,'' lastName
,a111.TeamName groupName
from
	SafariRpts.dbo.vwMSTR_ProvClm (nolock) pcl --Pre-invoice header record
	inner join SafariRpts.dbo.vwMSTR_TeamInsXref a17 with (nolock) on (pcl.ARContractKey = a17.ContractKey)
	inner join SafariRpts.dbo.vwMSTR_Team a111 with (nolock) on (a17.TeamKey = a111.TeamKey)
	left join SafariRpts.dbo.vwMSTR_InsClm (nolock) icl on pcl.ClaimId = icl.ClaimId --Post-invoice header record
	left join SafariRpts.dbo.vwMSTR_Contract (nolock) ctr on pcl.arContractKey = ctr.ContractKey
	left join SafariRpts.dbo.vwMSTR_PaymentType (nolock) pay on pcl.PaymentType = pay.PaymentType
	left join SafariRpts.dbo.vwMSTR_Insurer (nolock) ins on ctr.InsKey = ins.InsKey
	left join SafariRpts.dbo.vwMSTR_PatPolicy (nolock) pat on (pcl.PatPolicyKey = pat.PatPolicyKey)
	left join (
				select distinct
					d.PatPolicyKey
					,max(d.PatClmTxDate) as PatClmTxDate
				from
					SafariRpts.dbo.vwMSTR_PatClaimTrans (nolock) d
				group by
					d.PatPolicyKey
				) pax on (pat.PatPolicyKey = pax.PatPolicyKey)	
	left join SafariRpts.dbo.vwMSTR_RAMWrittenBy2 (nolock) ram on (pat.WrittenBy = ram.WrittenBy)
	left join SafariRpts.dbo.vwMSTR_ProvCBOXref (nolock) xrf on (pcl.ProvKey = xrf.ProvKey)
	left join SafariRpts.dbo.vwMSTR_CBO (nolock) cbo on (xrf.CBOKey = cbo.CBOKey)
	left join SafariRpts.dbo.vwMSTR_Provider (nolock) pro on (pcl.ProvKey = pro.ProvKey)
	left join SafariRpts.dbo.vwMSTR_Territory (nolock) ter on (pro.RegionKey = ter.TerritoryKey)
	left join SafariRpts.dbo.vwMSTR_ProvRegion (nolock) prg on (ter.RegionKey = prg.RegionKey)
	left join SafariRpts.dbo.vwMSTR_Division (nolock) div on (prg.DivKey = div.DivKey)
	left join SafariRpts.dbo.vwMSTR_ErrType (nolock) err on (pat.ErrType = err.ErrType) --Overpayment reasons
	left join SafariRpts.dbo.vwMSTR_ProvClmDenialSubmission (nolock) den on (pcl.ProvClmHdrKey = den.ProvClmHdrKey)
	left join SafariRpts.dbo.vwMSTR_ProvClmHdrNoteMaxDEN (nolock) dnt on (pcl.ProvClmHdrKey = dnt.ProvClmHdrKey)
	left join SafariRpts.dbo.vwMSTR_ProvDenialReasonCtgy (nolock) dcg on (den.PDENDenialReasonCtgy = dcg.ProvDenialReason)
	left join SafariRpts.dbo.vwMSTR_ProvClmSuspenseCurr (nolock) sus on (pcl.ProvClmHdrKey = sus.ProvClmHdrKey)
	left join SafariRpts.dbo.vwMSTR_SuspenseReasonType (nolock) sur on (sus.SuspenseReasonType = sur.SuspenseReasonType)
	left join (
				select
					ProvClmHdrKey
					,max(ProvTxAmt) as OrigAmt
				from
					SafariRpts.dbo.vwMSTR_ProvClmDet (nolock)
				where
					TxCode = 'REFDUE'
				group by
					ProvClmHdrKey
				) pcd on pcl.ProvClmHdrKey = pcd.ProvClmHdrKey
	left join (
				select distinct
					a.ProvClmHdrKey
					,max(c.ProvTxRsnDescr) as ProvTxRsnDescr
					,max(a.CRDateAdded) as CRDateAdded
				from
					SafariRpts.dbo.vwMSTR_APChkRequest (nolock) a
					left join SafariRpts.dbo.vwMSTR_ProvClmDenialSubmission (nolock) b on (a.ProvClmHdrKey = b.ProvClmHdrKey)
					left join SafariRpts.dbo.vwMSTR_ProvTxRsnMaster (nolock) c on (a.ProvTxRsnMasterKey = c.ProvTxRsnMasterKey)
				where
					a.CommActionType = 'PDEN'
					and b.ProvClmHdrKey is null
				group by
					a.ProvClmHdrKey
				) chk on (pcl.ProvClmHdrKey = chk.ProvClmHdrKey)
where
	(
		(
		ctr.ServiceType in ('CB','PPN')
		and ctr.ClientType in ('STD','EPN')
		)
		or
		(
		pcl.ServiceType = 'DM'
		and pcl.CollProtocolType = 'PPN'
		)
		or ctr.contractdescr like '%PPN%'
	)
	and pcl.ProvClmFinStatus &lt;&gt; 'RC'
	and pcl.PostInvRecode = 0
	and pat.WrittenBy &lt;&gt; '10325'
	and div.DivName is not null
	and (
		pcl.RecoveryIdDate &gt;= @startDate -- '4/1/2019'
		and pcl.recoveryIdDate&lt;@endDate
		)
	and pcl.ServiceType = 'cb'
	and ctr.ClientType = 'std'</property>

                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Preliminary = 0
-- Final = 1
declare @dateOffset int = 0
declare @startDate datetime = dateadd(m,datediff(m,0,getdate())-@dateOffset,0)
declare @endDate datetime = dateadd(m,1,@startDate)

select 	'Credit Balance' lobDescription
,dateadd(m,datediff(m,0,a11.DateProvPaid),0) dateNum
,'Paid $' inflectionPoint
,a11.ClaimId
,a11.DateProvPaid kpiStatusDate
,case 
	when a15.ContractCode in ('113060004','113060005','113060006','113060007','113060010','113060011','113060012') then abs(a11.ProvPmts + a11.ProvAdjs)*0.5 
	else abs(a11.ProvPmts + a11.ProvAdjs) 
	END amount
,ram.WrittenBy
,ram.RAMWFullName
,a15.ContractDescr clientCode
,a11.paymentType recoveryTypeCode
,a18.PayTypeDescr recoveryType
,a110.TeamName username
,a110.TeamName firstName
,'' lastName
,a110.TeamName groupName
from safarirpts.dbo.vwMSTR_ProvClm	a11 with (nolock) 
join safarirpts.dbo.vwMSTR_Provider a12 with (nolock) on (a11.ProvKey = a12.ProvKey)
join safarirpts.dbo.vwMSTR_Territory a13 with (nolock) on (a12.RegionKey = a13.TerritoryKey)
join safarirpts.dbo.vwMSTR_ProvRegion a14 with (nolock) on (a13.RegionKey = a14.RegionKey)
join safarirpts.dbo.vwMSTR_Contract a15 with (nolock) on (a11.ARContractKey = a15.ContractKey)
join safarirpts.dbo.vwMSTR_TeamInsXref	a16 with (nolock) on (a11.ARContractKey = a16.ContractKey)
join safarirpts.dbo.vwMSTR_Division a17 with (nolock) on (a14.DivKey = a17.DivKey)
join safarirpts.dbo.vwMSTR_PaymentType	a18 with (nolock) on (a11.PaymentType = a18.PaymentType)
join safarirpts.dbo.vwMSTR_SubLOB a19 with (nolock) on (a15.ClientType = a19.SubLOB)
join safarirpts.dbo.vwMSTR_Team a110 with (nolock) on (a16.TeamKey = a110.TeamKey)
left join SafariRpts.dbo.vwMSTR_PatPolicy (nolock) pat on (a11.PatPolicyKey = pat.PatPolicyKey)
left join SafariRpts.dbo.vwMSTR_RAMWrittenBy2 (nolock) ram on (pat.WrittenBy = ram.WrittenBy)
where 1=1
and a11.DateProvPaid&gt;=@startDate
and a11.DateProvPaid&lt;@endDate
and a11.ProvClmFinStatus in ('ACT', 'AWA', 'APP', 'DEN', 'DREQ', 'INV', 'PQA', 'RTI', 'SUS')
and a11.PostInvRecode in (0)
and a19.SubLOBDescr = 'Standard Credit Balance'</property>

,dateadd(m,datediff(m,0,a11.DateStatusUpd),0) dateNum
,'Ready to Invoice' inflectionPoint
,a11.ClaimId
,a11.DateStatusUpd kpiStatusDate
,cast(1 as numeric(21,5)) amount
,ram.WrittenBy
,ram.RAMWFullName
,a15.ContractDescr clientCode
,a11.paymentType recoveryTypeCode
,pt.PayTypeDescr recoveryType
,a110.TeamName username
,a110.TeamName firstName
,'' lastName
,a110.TeamName groupName
from	vwMSTR_ProvClm	a11
	join	vwMSTR_Provider	a12
	  on 	(a11.ProvKey = a12.ProvKey)
	join	vwMSTR_Territory	a13
	  on 	(a12.RegionKey = a13.TerritoryKey)
	join	vwMSTR_ProvRegion	a14
	  on 	(a13.RegionKey = a14.RegionKey)
	join	vwMSTR_Contract	a15
	  on 	(a11.ARContractKey = a15.ContractKey)
	join	vwMSTR_TeamInsXref	a16
	  on 	(a11.ARContractKey = a16.ContractKey)
	join	vwMSTR_Division	a17
	  on 	(a14.DivKey = a17.DivKey)
	join	vwMSTR_TxOpen	a18
	  on 	(a15.Recoverable = a18.TxOpen)
	join	vwMSTR_SubLOB	a19
	  on 	(a15.ClientType = a19.SubLOB)
	join	vwMSTR_Team	a110
	  on 	(a16.TeamKey = a110.TeamKey)
join safarirpts.dbo.vwMSTR_PaymentType pt with (nolock) on (a11.PaymentType = pt.PaymentType)
left join SafariRpts.dbo.vwMSTR_PatPolicy (nolock) pat on (a11.PatPolicyKey = pat.PatPolicyKey)
left join SafariRpts.dbo.vwMSTR_RAMWrittenBy2 (nolock) ram on (pat.WrittenBy = ram.WrittenBy)
where	(a11.ProvClmFinStatus in ('RTI')
 and a15.ClientType in ('STD')
 and a11.DateStatusUpd&gt;=@startDate
 and a11.DateStatusUpd&lt;@endDate
 and a11.ServiceType in ('CB', 'PPN'))</property>
